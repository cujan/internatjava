<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.internatDB.data.Skola" table="skola" dynamic-insert="false" dynamic-update="false">
        <id name="id" type="integer">
            <column name="id"/>
            <generator class="identity"/>
        </id>
        <property name="nazov" type="string">
            <column name="nazov"/>
        </property>
        <property name="adresa" type="string">
            <column name="adresa"/>
        </property>
    </class>
</hibernate-mapping>